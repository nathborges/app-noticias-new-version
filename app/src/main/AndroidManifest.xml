<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ibm.newsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppFinal">
        <activity
            android:name=".TodasNoticiasActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".VideoActivity"
            android:exported="false" />
        <activity
            android:name=".CategoriasActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoticiaActivity"
            android:exported="false" />
        <activity
            android:name="com.ibm.newsapp.InicioActivity"
            android:exported="true" >
        </activity>
        <activity
            android:name=".SaibaMaisActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:screenOrientation="sensor">
        </activity>
    </application>

</manifest>